{
  "name": "serialize-query-params",
  "version": "1.3.3",
  "description": "A library for simplifying encoding and decoding URL query parameters.",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "lib/",
    "esm/"
  ],
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "npm run build:cjs && npm run build:es",
    "clean": "rimraf lib esm",
    "dev": "NODE_ENV=development tsc -w",
    "prepublishOnly": "npm run test && npm run clean && npm run build",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pbeshai/use-query-params.git"
  },
  "keywords": [
    "serialization",
    "serialize",
    "deserialize",
    "encode",
    "decode",
    "url",
    "query",
    "parameters",
    "query param"
  ],
  "author": "Peter Beshai <peter.beshai@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "15.0.1"
  },
  "peerDependencies": {
    "query-string": "^7"
  }
}
